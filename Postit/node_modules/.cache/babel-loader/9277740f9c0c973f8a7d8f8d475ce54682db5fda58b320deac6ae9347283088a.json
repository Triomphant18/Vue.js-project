lass=\"box-border md:box-content\">\n    <ul v-if=\"this.tasks.length > 0\">\n      <li v-for=\"(task, index) in tasks\" :key=\"index\">\n        <h2>{{ task.title }}</h2>\n        <br />\n        <div class=\"description\" v-for=\"(noteline, i) in task.content\" :key=\"i\">\n          {{ noteline }}\n        </div>\n        <div class=\"flex\">\n          <div>\n            <router-link :to=\"{ name: 'detail', params: {id:task._id} }\">\n              <img src=\"../assets/forward.png\" alt=\"Forward Icone\" />\n            </router-link>\n          </div>\n          <div>\n            <img\n              @click=\"deleteNote(index)\"\n              class=\"delete\"\n              src=\"../assets/delete.png\"\n              alt=\"Forward Icone\"\n            />\n          </div>\n        </div>\n      </li>\n      <div>\n        <div class=\"add\" v-if=\"!shownote\">\n          <li>\n            <div class=\"description add\">\n              <img\n                class=\"iconplus\"\n                src=\"../assets/iconsplus.png\"\n                alt=\"Forward Icone\"\n                @click=\"addnote\"\n              />\n            </div>\n          </li>\n        </div>\n        <div class=\"note\" v-if=\"shownote\">\n          <li>\n            <h2>\n              <input\n                v-model=\"task\"\n                class=\"hide h2\"\n                type=\"text\"\n                placeholder=\"Add the title\"\n                size=\"11\"\n              />\n            </h2>\n            <br />\n            <br />\n            <div class=\"hide\">\n              <textarea\n                v-model=\"desc description\"\n                name=\"\"\n                id=\"\"\n                cols=\"28\"\n                rows=\"7\"\n                class=\"areadescription\"\n                placeholder=\"Add a description\"\n              ></textarea>\n            </div>\n            <div class=\"flex\">\n              <img\n                v-if=\"shownote\"\n      