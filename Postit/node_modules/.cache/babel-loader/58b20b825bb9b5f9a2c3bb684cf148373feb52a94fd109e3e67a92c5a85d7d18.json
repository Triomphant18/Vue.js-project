ex\">\n          <div>\n            <img src=\"../assets/forward.png\" alt=\"Forward Icone\" />\n          </div>\n          <div>\n            <img\n              @dblclick=\"deleteNote\"\n              class=\"delete\"\n              src=\"../assets/delete.png\"\n              alt=\"Forward Icone\"\n            />\n          </div>\n        </div>\n      </li>\n      <div>\n        <div class=\"add\" v-if=\"!shownote\">\n          <li>\n            <div class=\"description add\">\n              <img\n                class=\"iconplus\"\n                src=\"../assets/iconsplus.png\"\n                alt=\"Forward Icone\"\n                @click=\"addnote\"\n              />\n            </div>\n          </li>\n        </div>\n        <div class=\"note\" v-if=\"shownote\">\n          <li>\n            <h2>\n              <input\n                v-model=\"task\"\n                class=\"hide h2\"\n                type=\"text\"\n                placeholder=\"Add the title\"\n                size=\"11\"\n              />\n            </h2>\n            <br />\n            <br />\n            <div class=\"hide\">\n              <textarea\n                v-model=\"desc\"\n                name=\"\"\n                id=\"\"\n                cols=\"28\"\n                rows=\"7\"\n                class=\"areadescription\"\n                placeholder=\"Add a description\"\n              ></textarea>\n            </div>\n            <div class=\"flex\">\n              <img\n                v-if=\"shownote\"\n                src=\"../assets/forward.png\"\n                class=\"backward\"\n                alt=\"Backward Icone\"\n                @click=\"addnote\"\n              />\n              <div>\n                <img\n                  @click=\"submitNote\"\n                  src=\"../assets/approval.png\"\n                  class=\"approuved\"\n                  alt=\"Forward Icone\"\n                />\n                <!-- <img src=\"../assets/forward.png\" class=\"confirm\" alt=\"Forward Icone\"/> -->\n              </div>\n            </div>\n          </li>\n        </div>\n      </div>\n    </ul>\n  </div>\n</template>\n\n\n<script>\nimport axios from \"axios\"\nexport default {\n  //components: { Cadran },\n  data() {\n    return {\n      currentDate() {\n        const current = new Date();\n\n        return date;\n      },\n      task: \"\",\n      desc: \"\",\n      tasks: [],\n      togglenote: true,\n      shownote: false,\n      addnotes: true,\n\n    };\n  },\n  mounted(){\n    this.getNotes();\n  },\n  methods: {\n    getNotes(){\n      axios.get(\"https://post-it-7m1i.onrender.com/notes\").then(res => {\n      console.log(res)\n      this.tasks = res.data.notes\n    });\n    },\n    addnote() {\n      this.shownote = !this.shownote;\n    },\n\n    deleteNote(index){\n      this.tasks.splice(index, 1);\n    }, \n    submitNote() {\n      if (this.task.length == 0 || this.desc.length == 0) {\n        return;\n      }\n      if (this.desc.length > 300) {\n        this.desc = this.desc.substring(0, 300) + \" . . .\";\n      }\n\n      this.tasks.push({\n        title: this.task,\n        description: this.desc,\n      });\n      this.shownote = !this.shownote;\n      this.task = \"\";\n      this.desc = \"\";\n    },\n  },\n};\n</script>\n\n"],"mappings":";AAwFA,OAAOA,KAAI,MAAO,OAAM;AACxB,eAAe;EACb;EACAC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,WAAWA,CAAA,EAAG;QACZ,MAAMC,OAAM,GAAI,IAAIC,IAAI,EAAE;QAE1B,OAAOC,IAAI;MACb,CAAC;MACDC,IAAI,EAAE,EAAE;MACRC,IAAI,EAAE,EAAE;MACRC,KAAK,EAAE,EAAE;MACTC,UAAU,EAAE,IAAI;MAChBC,QAAQ,EAAE,KAAK;MACfC,QAAQ,EAAE;IAEZ,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAE;IACP,IAAI,CAACC,QAAQ,EAAE;EACjB,CAAC;EACDC,OAAO,EAAE;IACPD,QAAQA,CAAA,EAAE;MACRb,KAAK,CAACe,GAAG,CAAC,yCAAyC,CAAC,CAACC,IAAI,CAACC,GAAE,IAAK;QACjEC,OAAO,CAACC,GAAG,CAACF,GAAG;QACf,IAAI,CAACT,KAAI,GAAIS,GAAG,CAAChB,IAAI,CAACmB,KAAI;MAC5B,CAAC,CAAC;IACF,CAAC;IACDC,OAAOA,CAAA,EAAG;MACR,IAAI,CAACX,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC,CAAC;IAEDY,UA