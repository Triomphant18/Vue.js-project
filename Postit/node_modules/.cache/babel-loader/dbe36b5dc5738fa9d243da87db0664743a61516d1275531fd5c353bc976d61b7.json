{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nexport default {\n  data() {\n    return {\n      shownote: false,\n      desc: '',\n      note: ''\n    };\n  },\n  name: 'Notes',\n  mounted() {\n    this.getNotes();\n  },\n  updated() {\n    this.getNotes();\n  },\n  methods: {\n    ...mapActions(['getNotes', 'submitNote', 'deleteNote', update]),\n    addNote() {\n      this.shownote = !this.shownote;\n    },\n    async onSubmit() {\n      this.shownote = !this.shownote;\n      if (this.note === '' || this.desc.trim() === '') {\n        return;\n      }\n      await this.submitNote({\n        title: this.note,\n        content: this.desc\n      });\n      this.note = '';\n      this.desc = '';\n    }\n  },\n  computed: {\n    ...mapGetters(['allNotes'])\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","data","shownote","desc","note","name","mounted","getNotes","updated","methods","update","addNote","onSubmit","trim","submitNote","title","content","computed"],"sources":["/home/triomphant/Documents/TITA/MVVC/test2/mypostit/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"box-border md:box-content\">\n    <div>\n      <ul>\n        <div class=\"add\" v-if=\"!shownote\">\n          <li>\n            <div class=\"description add\">\n              <img\n                class=\"iconplus\"\n                src=\"../assets/iconsplus.png\"\n                alt=\"Forward Icone\"\n                @click=\"addNote\"\n              />\n            </div>\n          </li>\n        </div>\n        <div class=\"note\" v-if=\"shownote\">\n          <li>\n            <h2>\n              <input\n                v-model=\"note\"\n                class=\"hide h2\"\n                type=\"text\"\n                placeholder=\"Add the title\"\n                size=\"16\"\n              />\n            </h2>\n            <br />\n            <div class=\"hide\">\n              <textarea\n                v-model=\"desc\"\n                name=\"\"\n                id=\"\"\n                cols=\"24\"\n                rows=\"7\"\n                class=\"areadescription\"\n                placeholder=\"Add a description\"\n              ></textarea>\n            </div>\n            <div class=\"flex\">\n              <img\n                src=\"../assets/forward.png\"\n                class=\"backward\"\n                alt=\"Backward Icone\"\n                @click=\"addNote\"\n              />\n              <div>\n                <img\n                  @click=\"onSubmit()\"\n                  src=\"../assets/approval.png\"\n                  class=\"approuved\"\n                  alt=\"Forward Icone\"\n                />\n              </div>\n            </div>\n          </li>\n        </div>\n\n        <ul v-if=\"allNotes.length > 0\">\n          <li v-for=\"note in allNotes\" :key=\"note._id\">\n            <h2>\n              {{\n                note.title.length > 12\n                  ? note.title.substring(0, 12) + '...'\n                  : note.title\n              }}\n            </h2>\n            <div\n              class=\"description\"\n              v-for=\"(noteline, i) in note.content\"\n              :key=\"i\"\n            >\n              {{\n                noteline.length > 280\n                  ? noteline.substring(0, 220) + '...'\n                  : noteline\n              }}\n            </div>\n            <div class=\"flex\">\n              <div>\n                <img src=\"../assets/forward.png\" alt=\"Forward Icone\" />\n              </div>\n              <div>\n                <img\n                  @click=\"deleteNote(note._id)\"\n                  class=\"delete\"\n                  src=\"../assets/delete.png\"\n                  alt=\"Forward Icone\"\n                />\n              </div>\n            </div>\n          </li>\n        </ul>\n        </ul>\n    </div>\n  </div>\n</template>\n<script>\nimport { mapGetters, mapActions } from 'vuex';\n\nexport default {\n  data() {\n    return {\n      shownote: false,\n      desc: '',\n      note: '',\n    };\n  },\n  name: 'Notes',\n  mounted() {\n    this.getNotes();\n  },\n  updated() {\n    this.getNotes();\n  },\n  methods: {\n    ...mapActions(['getNotes', 'submitNote', 'deleteNote', update]),\n\n    addNote() {\n      this.shownote = !this.shownote;\n    },\n    \n    async onSubmit() {\n      this.shownote = !this.shownote;\n      if (this.note === '' || this.desc.trim() === '') {\n        return;\n      }\n      await this.submitNote({\n        title: this.note,\n        content: this.desc,\n      });\n      this.note = '';\n      this.desc = '';\n    },\n  },\n  computed: {\n    ...mapGetters(['allNotes']),\n  },\n};\n</script>"],"mappings":"AAkGA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAE7C,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,IAAI,EAAE,EAAE;MACRC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,IAAI,EAAE,OAAO;EACbC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,EAAE;EACjB,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACD,QAAQ,EAAE;EACjB,CAAC;EACDE,OAAO,EAAE;IACP,GAAGT,UAAU,CAAC,CAAC,UAAU,EAAE,YAAY,EAAE,YAAY,EAAEU,MAAM,CAAC,CAAC;IAE/DC,OAAOA,CAAA,EAAG;MACR,IAAI,CAACT,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC,CAAC;IAED,MAAMU,QAAQA,CAAA,EAAG;MACf,IAAI,CAACV,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;MAC9B,IAAI,IAAI,CAACE,IAAG,KAAM,EAAC,IAAK,IAAI,CAACD,IAAI,CAACU,IAAI,EAAC,KAAM,EAAE,EAAE;QAC/C;MACF;MACA,MAAM,IAAI,CAACC,UAAU,CAAC;QACpBC,KAAK,EAAE,IAAI,CAACX,IAAI;QAChBY,OAAO,EAAE,IAAI,CAACb;MAChB,CAAC,CAAC;MACF,IAAI,CAACC,IAAG,GAAI,EAAE;MACd,IAAI,CAACD,IAAG,GAAI,EAAE;IAChB;EACF,CAAC;EACDc,QAAQ,EAAE;IACR,GAAGlB,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}