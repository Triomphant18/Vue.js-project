{"ast":null,"code":"import { mapGetters, mapActions } from 'vuex';\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      shownote: false,\n      desc: '',\n      note: ''\n    };\n  },\n  name: 'Notes',\n  mounted() {\n    this.getNotes();\n  },\n  methods: {\n    ...mapActions(['getNotes', 'submitNote', 'deleteNote']),\n    addNote() {\n      this.shownote = !this.shownote;\n    },\n    async onSubmit() {\n      if (this.note === '' || this.desc === '') {\n        return;\n      }\n      await this.submitNote({\n        title: this.note,\n        content: this.desc.split('\\n')\n      });\n      this.note = '';\n      this.desc = '';\n      this.shownote = false;\n    }\n  },\n  computed: {\n    ...mapGetters(['allNotes'])\n  },\n  created() {\n    this.getNotes();\n  }\n};","map":{"version":3,"names":["mapGetters","mapActions","axios","data","shownote","desc","note","name","mounted","getNotes","methods","addNote","onSubmit","submitNote","title","content","split","computed","created"],"sources":["/home/triomphant/Documents/TITA/MVVC/test2/mypostit/src/views/HomeView.vue"],"sourcesContent":["<template>\n  <div class=\"box-border md:box-content\">\n    <div>\n      <ul>\n        <div class=\"add\" v-if=\"!shownote\">\n          <li>\n            <div class=\"description add\">\n              <img\n                class=\"iconplus\"\n                src=\"../assets/iconsplus.png\"\n                alt=\"Forward Icone\"\n                @click=\"addote\"\n              />\n            </div>\n          </li>\n        </div>\n        <div class=\"note\" v-if=\"shownote\">\n          <li>\n            <h2>\n              <input\n                v-model=\"task\"\n                class=\"hide h2\"\n                type=\"text\"\n                placeholder=\"Add the title\"\n                size=\"16\"\n              />\n            </h2>\n            <br />\n            <div class=\"hide\">\n              <textarea\n                v-model=\"desc\"\n                name=\"\"\n                id=\"\"\n                cols=\"24\"\n                rows=\"7\"\n                class=\"areadescription\"\n                placeholder=\"Add a description\"\n              ></textarea>\n            </div>\n            <div class=\"flex\">\n              <img\n                v-if=\"shownote\"\n                src=\"../assets/forward.png\"\n                class=\"backward\"\n                alt=\"Backward Icone\"\n                @click=\"addnote\"\n              />\n              <div>\n                <img\n                  @click=\"onSubmit\"\n                  src=\"../assets/approval.png\"\n                  class=\"approuved\"\n                  alt=\"Forward Icone\"\n                />\n              </div>\n            </div>\n          </li>\n        </div>\n\n        <ul v-if=\"this.tasks.length > 0\">\n          <li v-for=\"(task, index) in tasks.reverse()\" :key=\"index\">\n            <h2>\n              {{\n                task.title.length > 12\n                  ? task.title.substring(0, 12) + \"...\"\n                  : task.title\n              }}\n            </h2>\n            <div\n              class=\"description\"\n              v-for=\"(noteline, i) in task.content\"\n              :key=\"i\"\n            >\n              {{\n                noteline.length > 280 ? noteline.substring(0, 220) + \"...\" : noteline\n              }}\n            </div>\n            <div class=\"flex\">\n              <div>\n                  <img src=\"../assets/forward.png\" alt=\"Forward Icone\" />\n              </div>\n              <div>\n                <img\n                  @click=\"deleteNote(note._id)\"\n                  class=\"delete\"\n                  src=\"../assets/delete.png\"\n                  alt=\"Forward Icone\"\n                />\n              </div>\n            </div>\n          </li>\n        </ul>\n      </ul>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters, mapActions } from 'vuex';\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      shownote: false,\n      desc: '',\n      note: '',\n    };\n  },\n  name: 'Notes',\n  mounted() {\n    this.getNotes();\n  },\n  methods: {\n    ...mapActions(['getNotes', 'submitNote', 'deleteNote']),\n    addNote() {\n      this.shownote = !this.shownote;\n    },\n    async onSubmit() {\n      if (this.note === '' || this.desc === '') {\n        return;\n      }\n      await this.submitNote({\n        title: this.note,\n        content: this.desc.split('\\n'),\n      });\n      this.note = '';\n      this.desc = '';\n      this.shownote = false;\n    },\n  },\n  computed: {\n    ...mapGetters(['allNotes']),\n  },\n  created() {\n    this.getNotes();\n  },\n};\n</script>\n"],"mappings":"AAkGA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,KAAK;MACfC,IAAI,EAAE,EAAE;MACRC,IAAI,EAAE;IACR,CAAC;EACH,CAAC;EACDC,IAAI,EAAE,OAAO;EACbC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,QAAQ,EAAE;EACjB,CAAC;EACDC,OAAO,EAAE;IACP,GAAGT,UAAU,CAAC,CAAC,UAAU,EAAE,YAAY,EAAE,YAAY,CAAC,CAAC;IACvDU,OAAOA,CAAA,EAAG;MACR,IAAI,CAACP,QAAO,GAAI,CAAC,IAAI,CAACA,QAAQ;IAChC,CAAC;IACD,MAAMQ,QAAQA,CAAA,EAAG;MACf,IAAI,IAAI,CAACN,IAAG,KAAM,EAAC,IAAK,IAAI,CAACD,IAAG,KAAM,EAAE,EAAE;QACxC;MACF;MACA,MAAM,IAAI,CAACQ,UAAU,CAAC;QACpBC,KAAK,EAAE,IAAI,CAACR,IAAI;QAChBS,OAAO,EAAE,IAAI,CAACV,IAAI,CAACW,KAAK,CAAC,IAAI;MAC/B,CAAC,CAAC;MACF,IAAI,CAACV,IAAG,GAAI,EAAE;MACd,IAAI,CAACD,IAAG,GAAI,EAAE;MACd,IAAI,CAACD,QAAO,GAAI,KAAK;IACvB;EACF,CAAC;EACDa,QAAQ,EAAE;IACR,GAAGjB,UAAU,CAAC,CAAC,UAAU,CAAC;EAC5B,CAAC;EACDkB,OAAOA,CAAA,EAAG;IACR,IAAI,CAACT,QAAQ,EAAE;EACjB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}